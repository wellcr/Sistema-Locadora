   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-action>#{vendedorBacking.consultaVendedor}</from-action>
   <to-view-id>/consultarVendedor.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-action>#{vendedorBacking.alteraVendedor}</from-action>
   <to-view-id>/alterarVendedor.jsp</to-view-id>

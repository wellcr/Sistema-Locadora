   <to-view-id>/efetuarDevolucao.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <!-- Regras de navegação da tela cadastra cliente -->
 <navigation-rule>
  <from-view-id>/cadastrarCliente.jsp</from-view-id>
  <navigation-case>
   <from-outcome>cancelar</from-outcome>
   <to-view-id>/index.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
